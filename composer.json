{
    "name": "minmax/laravel",
    "description": "The Laravel Project.",
    "keywords": ["framework", "laravel"],
    "license": "MinMax",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-gd": "*",
        "ext-json": "*",
        "barryvdh/laravel-elfinder": "^0.3.12",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "fideloper/proxy": "~4.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "league/fractal": "^0.17.0",
        "mcamara/laravel-localization": "^1.3",
        "phpoffice/phpspreadsheet": "^1.2",
        "santigarcor/laratrust": "5.0.*",
        "yajra/laravel-datatables-fractal": "^1.2",
        "yajra/laravel-datatables-oracle": "^8.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "~2.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Minmax\\Base\\": "packages/minmax/base/src/",
            "Minmax\\Io\\": "packages/minmax/io/src/",
            "Minmax\\World\\": "packages/minmax/world/src/",
            "Minmax\\Member\\": "packages/minmax/member/src/",
            "Minmax\\Newsletter\\": "packages/minmax/newsletter/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-elfinder"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
